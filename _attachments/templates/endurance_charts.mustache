    <fieldset id="filter">
      <legend>Filter</legend>
      <div id="branch-selection">
        <span>Branch:</span>
        <span>All</span>
        <span>11.0</span>
        <span>10.0</span>
        <span>9.0</span>
        <span>8.0</span>
        <span>3.6</span>
      </div>

      <div id="os-selection">
        <span>OS: </span>
        <span>All</span>
        <span>Linux</span>
        <span>Mac</span>
        <span>Windows NT</span>
      </div>

      <div id="date">
        <span>From: </span>
        <input id="start-date" class="datepicker" type="text" value="" size="11" />
        <span>To: </span>
        <input id="end-date" class="datepicker" type="text" value="" size="11" />
      </div>
    </fieldset>

    <h2>Endurance Tests</h2>

    <script>
        function convertDateToUTC(date) {
            return Date.UTC(date.getUTCFullYear(), date.getUTCMonth(), date.getUTCDate(), date.getUTCHours(), date.getUTCMinutes(), date.getUTCSeconds());
        }

        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'test-result-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            series: [{
                name: "Passed",
                data: [{{#reports}}
                    [convertDateToUTC(new Date("{{time_start}}")), {{tests_passed}}],
                {{/reports}}],
                color: "green"
            },{
                name: "Skipped",
                data: [{{#reports}}
                    [convertDateToUTC(new Date("{{time_start}}")), {{tests_skipped}}],
                {{/reports}}],
                color: "orange"
            },{
                name: "Failed",
                data: [{{#reports}}
                    [convertDateToUTC(new Date("{{time_start}}")), {{tests_failed}}],
                {{/reports}}],
                color: "red"
            }],
            title: {
                text: 'Test Results'
            },
            xAxis: {
                type: 'datetime'
            },
            yAxis: {
                title: {
                    text: 'Tests'
                },
                min: 0
            },
        });
    
        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'max-explicit-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            series: [{{#platform_reports}}{
                name: "{{name}}",
                data: [
                    {{#reports}}
                        [convertDateToUTC(new Date("{{time_start}}")), {{#memory}}{{#explicit}}{{max}}{{/explicit}}{{^explicit}}""{{/explicit}}{{/memory}}],
                    {{/reports}}],
            },{{/platform_reports}}],
            title: {
                text: 'Max Explicit Memory'
            },
            xAxis: {
                type: 'datetime'
            },
            yAxis: {
                title: {
                    text: 'Memory Usage (MB)'
                },
                min: 0
            },
        });

        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'max-resident-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            series: [{{#platform_reports}}{
                name: "{{name}}",
                data: [
                    {{#reports}}
                        [convertDateToUTC(new Date("{{time_start}}")), {{#memory}}{{#resident}}{{max}}{{/resident}}{{^resident}}""{{/resident}}{{/memory}}],
                    {{/reports}}],
            },{{/platform_reports}}],
            title: {
                text: 'Max Resident Memory'
            },
            xAxis: {
                type: 'datetime'
            },
            yAxis: {
                title: {
                    text: 'Memory Usage (MB)'
                },
                min: 0
            },
        });

        chart1 = new Highcharts.Chart({
            chart: {
                borderColor: '#AAAAAA',
                borderRadius: 10,
                borderWidth: 1,
                renderTo: 'duration-chart',
                defaultSeriesType: 'line'
            },
            credits: {
                enabled: false
            },
            series: [{{#platform_reports}}{
                name: "{{name}}",
                data: [
                    {{#reports}}
                        [convertDateToUTC(new Date("{{time_start}}")), {{duration}} / 1000],
                    {{/reports}}],
            },{{/platform_reports}}],
            title: {
                text: 'Duration'
            },
            xAxis: {
                type: 'datetime'
            },
            yAxis: {
                title: {
                    text: 'Duration (s)'
                },
                min: 0
            },
        });
    </script>

    <div id="test-result-chart" class="chart" style="float:left"></div>
    <div id="max-explicit-chart" class="chart"></div>
    <div id="duration-chart" class="chart" style="float:left"></div>
    <div id="max-resident-chart" class="chart"></div>
